@section styles{
    <link rel="stylesheet" href="~/assets/plugins/custom/prismjs/prismjs.bundle.css" />
    <link rel="stylesheet" href="~/assets/plugins/custom/datatables/datatables.bundle.css" />
    <link rel="stylesheet" href="~/lib/tempus-dominus/css/tempus-dominus.css" />
    <style>
        .ui-autocomplete {
            position: absolute;
            cursor: default;
            z-index: 99999 !important;
        }

        .modalminH {
            min-height: 350px
        }

        .form-check-secondary.form-check-custom.form-check-solid > .form-check-input:checked {
            background-color: var(--kt-primary-active) !important;
        }
    </style>
}

@section breadcrumb{
    <li class="breadcrumb-item text-muted">
        <a href="#" class="text-muted text-hover-primary">Administración CVV</a>
    </li>
    <li class="breadcrumb-item">
        <span class="bullet bg-gray-400 w-5px h-2px"></span>
    </li>
    <li class="breadcrumb-item">
        <a asp-action="Index" class="text-hover-primary">@ViewData["Title"]</a>
    </li>
    <li class="breadcrumb-item">
        <span class="bullet bg-gray-400 w-5px h-2px"></span>
    </li>
    <li class="breadcrumb-item">
        <a class="text-hover-primary">Cita @Model.Folio</a>
    </li>
    }


@section toolbar{
    <a class="btn btn-sm fw-bold btn-primary" id="btnCancelarCita" data-id="@Model.IdCita" data-folio="@Model.Folio">Cancelar Cita</a>
    <a class="btn btn-sm fw-bold btn-primary" id="btnGuardarDoc">Guardar</a>
    }
