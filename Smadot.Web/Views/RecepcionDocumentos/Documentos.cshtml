    Layout = "~/Views/Shared/_LayoutSmadot.cshtml";
    if (Model?.DataDocumentos != null)
    {
        ViewData["Title"] = "Recepción de Documentos";

    }
    else
    {
        ViewData["Title"] = "Recepción de Documentos";

    }

