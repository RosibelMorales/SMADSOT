    <div class="d-flex flex-lg-row-fluid">
        <!--begin::Content-->

        <img class="theme-light-show mx-auto start mw-100 w-150px w-lg-400px mb-10 " src="/assets/media/logos/smadot2.png" alt="" />
        <!--begin::Image-->
        <!--end::Image-->
        <!--begin::Image-->

        <img class="theme-light-show mx-auto end mw-100 w-150px w-lg-400px mb-10 " src="/assets/media/logos/smadot1.png" alt="" />
        <!--end::Image-->
        <!--end::Content-->
    </div>
</div>
<form role="form" id="form_registro" clase="clearfix">
    <div class="card shadow-sm mb-5">
        <div class="card-header d-flex align-items-center justify-content-between py-3">
            <h2 class="card-title m-0">
                Folio de la Cita: @Model?.Folio
            </h2>

            <!--begin::Toggle-->
            <a href="#" class="btn btn-sm btn-icon btn-color-primary btn-light btn-active-light-primary d-lg-none" data-bs-toggle="tooltip" data-bs-dismiss="click" data-bs-placement="top" id="kt_inbox_aside_toggle" aria-label="Toggle inbox menu" data-bs-original-title="Toggle inbox menu" data-kt-initialized="1">
                <i class="ki-duotone ki-burger-menu-2 fs-3 m-0"><span class="path1"></span><span class="path2"></span><span class="path3"></span><span class="path4"></span><span class="path5"></span><span class="path6"></span><span class="path7"></span><span class="path8"></span><span class="path9"></span><span class="path10"></span></i>
            </a>
            <!--end::Toggle-->
        </div>
        <div class="card-body">
            <h4>Datos de la Cita</h4>
            <input asp-for="IdCita" id="IdCita" hidden />
            <input asp-for="IdRecepcionDocumento" hidden />
            <div class="d-flex mb-6 mt-6 row">
                <div class="py-2 px-4 col-md-4 fv-row">
                    <div class="form-check">
                        <input class="form-check-input" type="checkbox" value="" @Html.Raw(string.IsNullOrEmpty(Model.NombrePersona) ?"checked":"") id="PersonaMoral" />
                        <label class="form-check-label" for="flexCheckDefault">
                            Persona Moral
                        </label>
                    </div>
                </div>

            </div>
            <div class="d-flex mb-6 mt-6 row">
                <div class="py-2 px-4 col-md-4 fv-row">
                    <label class="form-label required titulo-check">@Html.Raw(string.IsNullOrEmpty(Model.NombrePersona) ?"Razón Social: &nbsp;":"Nombre del Propietario: &nbsp;")</label>
                    @*   <div class="position-relative">
                    <input class="form-control form-control-sm input-uppercase" disabled asp-for="RazonSocial" type="text" />
                    </div>*@
                    <div class="position-relative">
                        <input class="form-control form-control-sm input-uppercase" asp-for="NombrePersona" type="text" />
                    </div>
                </div>
                <div class="py-2 px-4 col-md-4 fv-row">
                    <label class="form-label required">Nombre de Quien Verifica: &nbsp;</label>
                    <div class="position-relative">
                        <input class="form-control form-control-sm  input-uppercase" asp-for="NombreGeneraCita" type="text" />
                    </div>
                </div>
                <div class="py-2 px-4 col-md-4 fv-row">
                    <label class="form-label required">Placa: &nbsp;</label>
                    <div class="position-relative">
                        <input class="form-control form-control-sm  input-uppercase" asp-for="Placa" />
                    </div>
                </div>
            </div>
            <div class="d-flex mb-6 mt-6 row">
                <div class="py-2 px-4 col-md-4 fv-row">
                    <label class="form-label required">Serie de Vehículo: &nbsp;</label>
                    <div class="position-relative">
                        <input class="form-control form-control-sm input-uppercase" asp-for="Serie" type="text" name="Serie" id="Serie" />
                    </div>
                </div>
                <div class="py-2 px-4 col-md-4 fv-row">
                    <label class="form-label required">Marca: &nbsp;</label>
                    <div class="position-relative">
                        <select class="form-control form-select " data-control="select2" asp-for="Marca" asp-items="(List<SelectListItem>)ViewBag.Marcas" id="IdCatMarcaVehiculo" name="IdCatMarcaVehiculo">
                            <option value="">Seleccione...</option>
                        </select>
                        @* <input class="form-control form-control-sm input-uppercase" type="hidden" asp-for="Marca" name="Marca" id="Marca" /> *@
                    </div>
                </div>
                @if (!string.IsNullOrEmpty(Model?.VchLinea))
                {
                    <div class="py-2 px-4 col-md-4 fv-row">
                        <label class="form-label required">Modelo del Registro: &nbsp;</label>
                        <div class="position-relative">
                            <input type="text" value="@Model.VchLinea" class="form-control form-control-sm input-uppercase" readonly disabled />
                        </div>
                    </div>

                }

            </div>

            <div class="d-flex mb-6 mt-6 row">
                <div class="py-2 px-4 col-md-4  fv-row">
                    <label class="form-label required">Año Modelo: &nbsp;</label>
                    @if (Model.Anio != 0)
                    {
                        <div class="position-relative">
                            <input class="form-control form-control-sm" asp-for="Anio" type="text" name="Anio" id="Anio" />
                        </div>
                    }
                    else
                    {
                        <div class="position-relative">
                            <input class="form-control form-control-sm" asp-for="Anio" type="text" name="Anio" id="Anio" />
                        </div>
                    }
                </div>
                <div class="py-2 px-4 col-md-4 fv-row">
                    <label class="form-label required">Tipo de Combustible: &nbsp;</label>
                    <div class="position-relative">
                        <select class="form-control form-select" asp-for="IdTipoCombustible"
                                data-control="select2" asp-items="(List<SelectListItem>)ViewBag.Combustible">
                            <option value="">Seleccione...</option>
                        </select>
                    </div>
                </div>
                <div class="py-2 px-4 col-md-4 fv-row">
                    <label class="form-label required">Submarca: &nbsp;</label>
                    <div class="position-relative">
                        <select class="form-control form-control-sm form-select-sm"
                                name="Modelo" id="Modelo">
                            <option value="@Model.IdSubMarcaVehiculo">@Model.Modelo</option>
                        </select>
                        @* <input class="form-control form-control-sm  input-uppercase" readonly disabled asp-for="Modelo" type="text" name="Modelo" id="Modelo" /> *@
                    </div>
                </div>
            </div>
            <div class="d-flex row">
                <div class="py-2 px-4 mb-6 col-md-4 fv-row">
                    <label class="form-label required">Numero de Referencia Multa: &nbsp;</label>
                    <div class="position-relative">
                        <input class="form-control form-control-sm input-uppercase" asp-for="NumeroReferencia" />
                        <label><p class="text-danger">Si no aplica, ingrese "0"</p></label>
                        @* <input class="form-control form-control-sm  input-uppercase" readonly disabled asp-for="Modelo" type="text" name="Modelo" id="Modelo" /> *@
                    </div>
                </div>

                <div class="py-2 px-4 mb-6 col-md-4  fv-row">
                    <label class="form-label required">Color: &nbsp;</label>
                    <div class="position-relative">
                        <input class="form-control form-control-sm" asp-for="ColorVehiculo" type="text" name="Color" id="Color" />
                    </div>
                </div>
                <div class="py-2 px-4 mb-6 col-md-4  fv-row">
                    <label class="form-label required">Tipo Servicio: &nbsp;</label>
                    <div class="position-relative">
                        @if (Model.DataDocumentos != null)
                        {
                            <select class="form-control form-select" asp-items="(List<SelectListItem>)ViewBag.tServicio" asp-for="@Model.DataDocumentos.IdCatTipoServicio" name="IdTipoServicio" id="IdTipoServicio">
                                <option value="">Seleccione...</option>

                            </select>
                        }
                        else
                        {
                            <select class="form-control form-select" asp-items="(List<SelectListItem>)ViewBag.tServicio" name="IdTipoServicio" id="IdTipoServicio">
                                <option value="">Seleccione...</option>

                            </select>

                        }
                    </div>
                </div>

                <div class="py-2 px-4 mb-6 col-md-4  fv-row">
                    <label class="form-label required">Motivo de verificación: &nbsp;</label>
                    <div class="position-relative">
                        <select class="form-control form-select form-select-sm" data-control="select2" asp-items="(List<SelectListItem>)ViewBag.MotivosVer" asp-for="IdMotivoVerificacion">
                            <option value="">Seleccione...</option>
                        </select>
                    </div>
                </div>
                <div class="py-2 px-4 mb-6 col-md-4  fv-row " id="containerEstado">
                    <label class="form-label required">Entidad de Procedencia del Vehículo: &nbsp;</label>
                    <div class="position-relative">
                        <select class="form-control form-select form-select-sm" data-control="select2" asp-items="(List<SelectListItem>)ViewBag.Estados" asp-for="Estado">
                            <option value="">Seleccione...</option>
                        </select>
                    </div>
                </div>

                <div class="py-2 px-4 mb-6 col-md-4  fv-row">
                    <label class="form-label required">Folio Certificado Anterior: &nbsp;</label>
                    <div class="position-relative">
                        <input class="form-control form-control-sm" asp-for="FolioCertificadoAnterior" />
                    </div>
                </div>
                <div class="py-2 px-4 mb-6 col-md-4 fv-row d-none" id="conteinerFechaFacturacion">
                    <div class="form-group input-group-sm">
                        <label for="fechaFacturacion">Fecha de Facturación: &nbsp;<strong class="obligatorio">*</strong></label>
                        <div class="position-relative">
                            <input class="form-control" id="FechaFacturacion" name="FechaFacturacion" type="text" value="" />
                        </div>
                    </div>
                </div>
            </div>

            @* <div class="d-flex mb-6 mt-6 row ">
            <div class="py-2 px-4 col-md-4  fv-row" @Html.Raw(Model.IdTipoCombustible!= Combustible.Diesel ? "style=\"display:none;\"":"") id="dieselcont">
            <label class="form-label required">Diesel: &nbsp;</label>
            <div class="position-relative">
            <select class="form-control form-select" name="IdDiesel" id="IdDiesel"
            data-control="select2" asp-items="(List<SelectListItem>)ViewBag.Diesel">
            <option value="">Seleccione...</option>
            </select>
            </div>
            </div>
            <div class="py-2 px-4 col-md-4  fv-row " id="divSubDiesel">
            <label class="form-label required">SubDiesel: &nbsp;</label>
            <div class="position-relative">
            <select class="form-control form-select form-select-sm" name="IdSubDiesel" id="IdSubDiesel">
            <option value="">Seleccione...</option>
            </select>
            </div>
            </div>
            </div> *@

        </div>
    </div>
