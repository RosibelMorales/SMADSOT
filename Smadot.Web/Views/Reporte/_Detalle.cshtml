        <span class="fs-6 text-gray-700 fw-bold">Numero de Entrega</span>
        <div class="fw-semibold text-gray-400">@Model.NumeroEntrega</div>
    </div>
    <div class="border border-gray-300 border-dashed rounded min-w-100px w-100 py-2 px-4 me-6 mb-3">
        @{ var url1 = Model.UrlDocumento.Split("/")[Model.UrlDocumento.Split("/").Length - 1];
             <label class="control-label fw-bold">Documento</label><br />
                //<a class="btn btn-outline btn-outline-dashed me-2 mb-2 descargarDoc" data-url="@(Model.Id+"/"+url1)">
