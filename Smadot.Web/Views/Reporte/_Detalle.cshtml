    </div>
</div>
<div class="d-flex">
    <div class="border border-gray-300 border-dashed rounded min-w-100px w-100 py-2 px-4 me-6 mb-3">
        <span class="fs-6 text-gray-700 fw-bold">Fecha Inicial</span>
        <div class="fw-semibold text-gray-400">@Model.FechaInicial</div>
    </div>
    <div class="border border-gray-300 border-dashed rounded min-w-100px w-100 py-2 px-4 me-6 mb-3">
        <span class="fs-6 text-gray-700 fw-bold">Fecha Final</span>
        <div class="fw-semibold text-gray-400">@Model.FechaFinal</div>
    </div>
</div>
<div class="d-flex">
    <table class="table table-dark table-striped table-rounded border gy-7 gs-7">
        <thead>
            <tr class="fw-semibold fs-6 text-gray-900 border-bottom-2 border-gray-200">
                <th>Clave</th>
                <th>Tipo</th>
                <th>Cantidad*</th>
                <th>Serie Inicial*</th>
                <th>Serie Final*</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var items in values)
            {
                <tr>
                    <td>@items.ClaveCertificado</td>
                    <td> @items.NombreCatTipoReporte </td>
                    <td> @items.Cantidad </td>
                    <td> @items.SerieInicial </td>
                    <td> @items.SerieFinal </td>
                </tr>
            }
        </tbody>
    </table>
</div>