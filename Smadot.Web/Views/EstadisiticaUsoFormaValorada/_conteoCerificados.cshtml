@model List<Smadot.Models.Entities.SolicitudFormaValorada.Response.CertificadoMapeo>
@if (Model.Count == 0)
{
    <div class="row g-5 g-xl-5 flex-center py-4">
        <a href="javascript:void(0)" class="card hover-elevate-up shadow-sm parent-hover">
            <div class="card-body d-flex align-items">

                <span class="ms-3 text-gray-700 parent-hover-primary fs-5 fw-bold">
                    No hay nada que mostrar
                </span>
            </div>
        </a>
    </div>
}
@foreach (var item in Model)
{
    <div class="col-lg-3 col-3">
        <label class="btn btn-outline btn-outline-dashed btn-active-light-primary  d-flex align-items-center mb-5 card-folio"
            for="kt_create_account_form_account_type_personal">
            <div class="symbol symbol-65px symbol-circle mb-3 me-4">
                <span class="symbol-label fs-2x fw-semibold @Html.Raw(item.ClassNames)">@item.Simbolo</span>
            </div>
            <!--begin::Info-->
            <span class="d-block fw-semibold text-start">
                <span class="text-dark fw-bold d-block fs-4 mb-2">@item.Nombre</span>

                <span class="text-dark d-block fs-8">Total: <span class="text-dark d-block fs-4"
                        id="FolioDobleCero">@(item.Total)</span></span>
            </span>
            <!--end::Info-->
        </label>
        <!--end::Option-->
        <div class="fv-plugins-message-container invalid-feedback"></div>
    </div>

}