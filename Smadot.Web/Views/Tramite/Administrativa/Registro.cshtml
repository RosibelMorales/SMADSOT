@model Smadot.Models.Entities.Administrativa.Request.AdministrativaRequestData.AdministrativaApiRequest

@{
    ViewData["Title"] = "Registrar Administrativa";
    Layout = "~/Views/Shared/_LayoutSmadot.cshtml";
}

@section styles{
    @*<link rel="stylesheet" href="~/lib/datatable/dist/css/dataTables.bootstrap4.min.css" />*@
    <link rel="stylesheet" href="~/lib/jquery-ui/dist/jquery-ui.min.css" type="text/css" />
    <style>
        .ui-autocomplete {
            position: absolute;
            cursor: default;
            z-index: 99999 !important;
        }
    </style>
}

@section breadcrumb{
    <li class="breadcrumb-item text-muted">Trámites</li>
    <li class="breadcrumb-item text-muted">
        <span class="bullet bg-gray-400 w-5px h-2px"></span>
    </li>
    <li class="breadcrumb-item text-muted">Certificados</li>
    <li class="breadcrumb-item text-muted">
        <span class="bullet bg-gray-400 w-5px h-2px"></span>
    </li>
    <li class="breadcrumb-item text-muted">
        <a asp-controller="Administrativa" asp-action="Index" class="text-muted text-hover-primary">Administrativa</a>
    </li>
    <li class="breadcrumb-item text-muted">
        <span class="bullet bg-gray-400 w-5px h-2px"></span>
    </li>
    <li class="active">Registrar Administrativa</li>
}

@section toolbar{
}

