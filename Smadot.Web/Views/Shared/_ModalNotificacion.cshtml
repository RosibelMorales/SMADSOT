@model String
<!-- Modal-->
<div class="modal fade" id="modalNotificacion" tabindex="-1" role="dialog" aria-labelledby="modalRegistro" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-lg modal-dialog-scrollable" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="TituloModalNotificacion">@Model</h5>               
                <button class="btn btn-icon w-35px h-20px btnCerrarModalRegistro" data-bs-dismiss="modal" aria-label="Close">
                    <span class="bi bi-x"></span>
                </button>
            </div>
            <div class="modal-body" id="ModalNotificacionBody">
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-sm btn-primary me-2 btnCerrarModalRegistro" id="btnCerrarModalRegistro" data-bs-dismiss="modal">Cerrar</button>
            </div>
        </div>
    </div>
</div>