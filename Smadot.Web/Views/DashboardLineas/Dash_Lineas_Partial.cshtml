                    <a href="javascript:;" data-bs-toggle="dropdown">
                        <span class="fs-4 fw-bold text-gray-900 text-hover-primary me-1 mb-2 lh-1">Camara <i class="bi bi-camera-video"></i></span>
                    </a>
                    @*  <ul class="dropdown-menu">
                <li><h6 class="dropdown-header">Selecciona una acción:</h6></li>
                <li>
                <a class="btnDetalle">
                <label class="dropdown-item">
                <span class="navi-icon"><i class="bi bi-search"></i></span>&nbsp;&nbsp;
                <span class="navi-text">Ver</span>
                </label>
                </a>
                </li>
                </ul>*@

                    <div class="mb-0 lh-1" style="margin-top: 4px;">
                        <span class="badge badge-success badge-circle w-10px h-10px me-1"></span>
                        <span class="fs-7 fw-semibold text-muted">Activada</span>
                    </div>
                }
                else
                {
                    <span class="fs-4 fw-bold text-gray-900 text-hover-primary me-1 mb-2 lh-1">Camara <i class="bi bi-camera-video"></i></span>
                    <div class="mb-0 lh-1">
                        <span class="badge badge-danger badge-circle w-10px h-10px me-1"></span>
                        <span class="fs-7 fw-semibold text-muted">Desactivada</span>
                    </div>
                }
                <!--end::Info-->
            </div>
            <div class="d-flex justify-content-center flex-column me-3">
                <a href="javascript:;" data-bs-toggle="dropdown">
                    <span class="fs-4 fw-bold text-gray-900 text-hover-primary me-1 mb-2 lh-1">Linea <i class="bi bi-chevron-double-down"></i></span>
                </a>
                <ul class="dropdown-menu">
                    <li><h6 class="dropdown-header">Selecciona una acción:</h6></li>
                    <li>
                        @if (!@Model.Line)
                        {
                            <a class="btnOpen" data-id="@Model.IdLinea">
                                <label class="dropdown-item">
                                    <span class="navi-icon"><i class="bi bi-toggle-on"></i></span>&nbsp;&nbsp;
                                    <span class="navi-text">Abrir</span>
                                </label>
                            </a>
                        }
                        else
                        {
                            <a class="btnClose" data-id="@Model.IdLinea">
                                <label class="dropdown-item">
                                    <span class="navi-icon"><i class="bi bi-toggle-off"></i></span>&nbsp;&nbsp;
                                    <span class="navi-text">Cerrar</span>
                                </label>
                            </a>
                        }
                    </li>
                </ul>
                <!--begin::Info-->
                @if (@Model.Line)
                {
                    <div class="mb-0 lh-1">
                        <span class="badge badge-success badge-circle w-10px h-10px me-1"></span>
                        <span class="fs-7 fw-semibold text-muted">Activada</span>
                    </div>
                }
                else
                {
                    <div class="mb-0 lh-1">
                        <span class="badge badge-danger badge-circle w-10px h-10px me-1"></span>
                        <span class="fs-7 fw-semibold text-muted">Desactivada</span>
