                        </div>


                        @if (Model.CRechazo != CausaRechazo.NoAplica)
                        {
                            <div class="flex-fill">
                                <span class="text-danger fw-bold d-block fs-6">Causa: @CausaRechazo.DictClaveRechazo[Model.CRechazo]</span>
